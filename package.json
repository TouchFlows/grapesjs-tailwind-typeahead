{
  "name": "grapesjs-tailwind-typeahead",
  "version": "1.6.0",
  "description": "Grapesjs Tailwind Typeahead Plugin",
  "main": "dist/grapesjs-tailwind-typeahead.umd.js",
  "module": "dist/grapesjs-tailwind-typeahead.mjs",
  "unpkg": "./dist/grapesjs-tailwind-typeahead.umd.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/grapesjs-tailwind-typeahead.mjs",
      "require": "./dist/grapesjs-tailwind-typeahead.cjs"
    },
    "./dist/grapesjs-tailwind-typeahead.min.css": "./dist/grapesjs-tailwind-typeahead.min.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/touchflows/grapesjs-tailwind-typeahead.git"
  },
  "bugs": {
    "url": "https://github.com/touchflows/grapesjs-tailwind-typeahead/issues"
  },
  "homepage": "https://github.com/touchflows/grapesjs-tailwind-typeahead#readme",
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "tsc & vite build",
    "build:debug": "tsc & vite build --debug",
    "preview": "vite preview",
    "bump": "npm version patch -m 'Release v%s'"
  },
  "keywords": [
    "grapesjs",
    "plugin",
    "typeahead"
  ],
  "peerDependencies": {
    "typeahead-standalone": "^4.21.0",
    "grapesjs": "^0.21.2"
  },
  "devDependencies": {
    "@types/node": "^20.2.5",    
    "rollup-plugin-typescript-paths": "^1.4.0",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vite-plugin-dts": "^2.3.0"
  },
  "author": "Mark Tamis",
  "license": "MIT",
  "dependencies": {
    "tailwindcss": "^3.3.2"
  }
}
