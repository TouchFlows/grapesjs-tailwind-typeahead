{
  "name": "grapesjs-tailwind-typeahead",
  "version": "1.3.7",
  "description": "Grapesjs Tailwind Typeahead Plugin",
  "type": "module",
  "main": "dist/umd/index.js",
  "module": "dist/es/index.js",
  "unpkg": "./dist/umd/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/grapesjs-tailwind-typeahead.js",
      "require": "./dist/grapesjs-tailwind-typeahead.umd.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/touchflows/grapesjs-tailwind-typeahead.git"
  },
  "bugs": {
    "url": "https://github.com/touchflows/grapesjs-tailwind-typeahead/issues"
  },
  "homepage": "https://github.com/touchflows/grapesjs-tailwind-typeahead#readme",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "bump": "npm version patch -m 'Release v%s'"
  },
  "keywords": [
    "grapesjs",
    "plugin"
  ],
  "devDependencies": {
    "@types/node": "^20.2.3",
    "grapesjs": "^0.21.1",
    "rollup-plugin-typescript-paths": "^1.4.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.8",
    "vite-plugin-dts": "^2.3.0"
  },
  "author": "Mark Tamis",
  "license": "MIT"
}
